{
  "name": "corporate-governance-vue",
  "version": "1.0.0",
  "description": "Corporate Governance Application with FHE - Confidential Shareholder Voting System built with Vue.js",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3001",
    "build": "vue-tsc --noEmit --skipLibCheck && vite build",
    "build:production": "vite build",
    "preview": "vite preview --host 0.0.0.0 --port 3019",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.ts --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "ethers": "6.14.3",
    "pinia": "^2.1.7",
    "fhevmjs": "^0.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "typescript": "^5.2.2",
    "vue-tsc": "^2.0.6",
    "vite": "^5.2.0",
    "vite-plugin-node-polyfills": "^0.22.0",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
